    "amqp-connection-manager": "^3.7.0",
    "amqplib": "^0.8.0",
    "helmet": "^4.6.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.4.0",
    "swagger-ui-express": "4.1.6"
  },
  "devDependencies": {
    "@nestjs/testing": "8.2.3",
